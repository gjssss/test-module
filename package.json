{
  "name": "nuxt-module",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.11.0",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "build:stub": "pnpm --filter \"./packages/**\" build:stub",
    "dev:prepare": "pnpm --filter \"./packages/**\" dev:prepare",
    "play:prepare": " pnpm --filter playground prepare",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.2.2",
    "@nuxt/devtools": "^1.0.4",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@types/node": "^20.10.3",
    "changelogen": "^0.5.5",
    "core": "workspace:*",
    "eslint": "^8.55.0",
    "lint-staged": "^15.2.0",
    "nuxt": "^3.8.2",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.2",
    "vitest": "^0.33.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
